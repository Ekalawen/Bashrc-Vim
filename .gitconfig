[user]
	email = mail@gmail.com
	name = My Name

[push]
	default = simple
[pull]
    rebase = true

[alias]
	c = checkout
	m = commit
	s = status
	cp = cherry-pick
	b = branch
	l = log --pretty=format:\"%C(yellow)%h%Creset %ad - %s%Cred%d%Creset %Cblue[%an]%Creset\" --date=short
	ls = "!git l --graph"
	ll = "!git l --numstat"
